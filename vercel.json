{
  "redirects": [
    {
      "source": "/",
      "missing": [
        {
          "type": "query",
          "key": "mode"
        }
      ],
      "destination": "/?mode=library",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
