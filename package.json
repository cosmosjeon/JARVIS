{
  "name": "hierarchical-force-tree-react",
  "version": "1.0.0",
  "description": "React-based hierarchical force-directed tree with hover animations",
  "main": "electron/main.js",
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:renderer": "cross-env BROWSER=none react-scripts start",
    "start:electron": "wait-on tcp:3000 && electron .",
    "electron:dev": "concurrently \"npm run start:renderer\" \"npm run start:electron\"",
    "electron:build": "npm run build && electron-builder",
    "electron:smoke": "node scripts/smoke/run-electron-smoke.js"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@supabase/supabase-js": "^2.58.0",
    "@types/d3": "^7.4.3",
    "ai": "^5.0.60",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.8.5",
    "dotenv": "^16.4.5",
    "electron-log": "^5.1.2",
    "framer-motion": "^10.16.16",
    "harden-react-markdown": "^1.1.2",
    "katex": "^0.16.22",
    "lucide-react": "^0.544.0",
    "openai": "^4.59.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.6.6",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "shiki": "^3.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-stick-to-bottom": "^1.1.1",
    "web-highlighter": "^0.7.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^31.2.0",
    "electron-builder": "^25.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.jarvis.widget",
    "asar": true,
    "files": [
      "build/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "extraMetadata": {
      "main": "electron/main.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
